{
  "name": "sif-webapp",
  "private": true,
  "version": "8.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host 0.0.0.0 --mode development",
    "preview": "vite preview",
    "test": "vite --port 3000 --host 0.0.0.0 --mode test",
    "build": "npx vite build --mode development",
    "open:android": "vite build --mode production && npx cap sync android && npx cap open android",
    "open:ios": "vite build --mode production && npx cap sync ios && npx cap open ios",
    "build:icons": "npx @capacitor/assets generate --iconBackgroundColor '#183E3B' --splashBackgroundColor '#183E3B'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:update": "cypress run --env updateSnapshots=true",
    "prettier": "npx prettier  .",
    "prettier:fix": "npx prettier --write",
    "eslint:fix": "eslint --fix",
    "typecheck": "tsc",
    "typecheck:staged": "tsc-files",
    "lint": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
